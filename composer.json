{
    "name": "opendialogai/opendialog",
    "type": "project",
    "description": "OpenDialog Demonstration App",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.4.0",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/laravel-caffeine": "^8.0",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "maennchen/zipstream-php": "^2.0",
        "opendialogai/core": "dev-feature/OPNDLG-885--virtual-intents",
        "opendialogai/dgraph-docker": "21.03.0.2",
        "opendialogai/webchat": "1.0.0",
        "phalcongelist/php-diff": "^2.0",
        "predis/predis": "^1.1",
        "spatie/commonmark-highlighter": "^2.0",
        "spatie/laravel-flash": "^1.3",
        "symfony/property-access": "^5.2",
        "symfony/serializer": "^5.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.7",
        "filp/whoops": "^2.5",
        "friendsofphp/php-cs-fixer": "^2.18",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^5.3",
        "phpro/grumphp": "^0.18.1",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=dgraph --force",
            "@php artisan vendor:publish --tag=public --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=dgraph --force",
            "@php artisan vendor:publish --tag=public --force"
        ]
    },
    "repositories":  [
        {
            "type": "vcs",
            "url": "https://github.com/opendialogai/google-cloud-php-dialogflow"
        }
    ]
}
